{
  "manifest_version": 2,

  "name": "weekly",
  "description": "Weekly to do list using chrome extension",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "static/icon.png"
  }
}
